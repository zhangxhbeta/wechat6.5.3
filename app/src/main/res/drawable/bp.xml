<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ca" />
            <solid android:color="@color/c1" />
            <stroke android:width="@dimen/cb" android:color="@color/c4" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ca" />
            <solid android:color="@color/c3" />
            <stroke android:width="@dimen/cb" android:color="@color/c6" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/ca" />
            <solid android:color="@color/c2" />
            <stroke android:width="@dimen/cb" android:color="@color/c5" />
        </shape>
    </item>
</selector>